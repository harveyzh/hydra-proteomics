<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="org.proteios.core.data.keyring.GroupKeys"
		table="`GroupKeys`"
		mutable="false"
		lazy="false"
	>
		<composite-id>
			<key-property
				name="groupId"
				column="`group_id`"
				type="int"
				access="property"
			/>
			<key-property
				name="keyId"
				column="`key_id`"
				type="int"
				access="property"
			/>
		</composite-id>
		<property
			name="permission"
			column="`permission`"
			type="int"
			not-null="true"
			access="property"
		/>
	</class>
</hibernate-mapping>
